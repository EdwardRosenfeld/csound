int
pyexec_krate(CSOUND *csound, PYEXEC *p)
{
  char source[1024];
  PyObject *result;

  strcpy(source, (char *)p->string);

  result = exec_file_in_given_context(source, 0);
  if (result == NULL)
    {
      csound->Message(csound, "pyexec_krate: python exception\n");
      PyErr_Print();
      return NOTOK;
    }
  Py_DECREF(result);
  return OK;
}

